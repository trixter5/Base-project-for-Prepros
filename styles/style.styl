@import 'nib';
@import 'flexbox-grid-stylus/src/flexbox-grid.styl';
@import 'vars';
@import 'fonts';
normalize-css()
// custom flexbox-grid-stylus container
.grid--container {
	max-width 1520px
}

//colors

$denim-50 = rgba(59, 75, 123, 0.5)
$pinkish-red = #ed2a48
$pale-grey = #f0f1f3
$white-six-50 = rgba(255, 255, 255, 0.5)
$blue-grey = #838ca5
$denim = #3b4b7b
$pinkish-red-two = #ed2a47
$denim--30 = rgba(59, 75, 123, 0.3)
$dark-indigo-20 = rgba(13, 37, 68, 0.2)
$bluey-grey = #a5abbd
$dark-indigo--40 = rgba(13, 37, 68, 0.4)
$dark-grey-blue = #2f3354
$light-blue-grey-three = #c9d5e9
$dusk = #4b5374
$blue-grey-two = #787da9
$dull-teal = #62a0a6
$straw = #fcf47a
$straw-70 = rgba(252, 244, 122, 0.2)
$white-six-60 = rgba(255, 255, 255, 0.2)
$light-blue-grey-four = #ddd4ef
$very-light-pink = #ffd8d4
$midnight-blue-60 = rgba(6, 0, 69, 0.2)
$very-light-pink-60 = rgba(254, 216, 213, 0.2)
$beige-two = #f0e0d1
$pale-grey-two = #e9f3f4
$light-blue-grey-60 = rgba(221, 212, 238, 0.2)
$silver = #c5c8d3
$denim-60 = rgba(61, 78, 128, 0.6)
body
	background-color #f0f1f3
.content--xs-right
	display flex
	flex-direction column
	justify-content flex-start
	align-items flex-start
	margin-top 0px
.img-responsive
	max-width 100% 
.table-responsive
	overflow hidden
	overflow-x auto
	table
		width 100%
.section
	padding 30px 0
	
.section--block-1
		h2
			font-size 28px
			line-height 30px
		h1
			font-size 34px
			line-height 38px
	.section--block-1 h1 + span
		font-size 18px
		line-height 20px
	.section--block-1 .btns-group
		flex-direction column
		margin-top 40px
		
		.btn
			margin-bottom 20px
			height 50px
			padding 15px 30px
			line-height 25px
.section--block-1__content
	text-align center
.section--block-1
	background url('../images/park-bg.jpg') 0 0 no-repeat
	color #fff
	padding-top 30px
	padding-bottom 30px
	background-size cover
	.btn
		display inline-block
	h2
		font-family bold
		font-family bold
		margin-top 0
	h1
		color #fff
		font-family bold
		text-transform none
	h1 + span
		letter-spacing 0.6px
		font-family bold
	.btns-group
		display flex
		align-items center
		justify-content flex-start
		.btn.btn--fill
			color #fff
			background-color $pinkish-red
			font-size 16px 
			text-align center
			line-height 25px
			
		.btn--glass
			background-color transparent
			border solid 1px #fff
		.btn
			border-radius  3px
			display inline-block
			box-sizing border-box
			font-family bold
			max-width 245px
.section--block-1__days
	height 89px
	width 209px
	border 2px solid #fff
	border-radius 3px
	box-sizing border-box
	font-size 60px
	text-align center
	letter-spacing 28.8px
	display flex
	align-items stretch
	padding 1px
	justify-content space-between
	margin auto
	
	div
		text-align center
		line-height 89px
		display inline-block
		padding 0 10px
		flex 0 0 64px
		min-width 64px
		box-sizing border-box
		&:nth-child(2)
			border-left 2px solid #fff
			border-right 2px solid #fff
.section--block-1__days-descriptor
	clear both
	display inline-block
	font-size 20px
	opacity 0.5
	line-height 1.35
	text-align right
	letter-spacing normal
	margin-top 17px
	max-width 173px
	white-space pre-wrap
	margin 20px auto
	text-align center
.section--block-2
	h2
		font-size 36px
		font-family bold
		color $denim-50
		line-height .96
		text-align center
		margin-top 0
		margin-bottom 30px
	.section--block-2__subhead
		color $denim
		font-family bold
		font-size 20px
		line-height 1.3
		text-align center
		max-width 1060px
		margin 0 auto
		margin-bottom 35px
.section--block-2__box-content
	padding-top 35px
	margin-left 0px
	
	p
		color $blue-grey
		font-size 16px
		line-height 24px
		margin-top 0
.section--block-2__box
	box-shadow 0px -10px 45px 0 rgba(13,37,68,0.1)
	padding 10px 10px 0
	background #fff
	.row
		box-shadow 0px 8px 10px -5px rgba(13,37,68,0.08)
.section--block-2__box-list
	margin-top 25px
	padding-top 33px
	margin-left 0px
	position relative
	&:before
		content ''
		absolute(top 0 left 0)
		width 77px
		height 3px
		background-color $pinkish-red-two
.section--block-2__box-row
	display flex
	justify-content space-between
	align-items flex-start
	margin-bottom 30px
	max-width 465px
	span
		&:first-child
			flex 1 2 auto
			font-size 25px
			line-height 80%
			color $denim--30
		&:nth-child(2)
			flex 2 3 50%
			color $denim
			font-size 16px
			line-height 1
		&:last-child
			flex 2 1 auto
			font-family bold
			span
				text-align right
				font-size 16px
				line-height 16px
				display block
				&:first-child
					color $pinkish-red-two
				&:last-child
					color $blue-grey
.section--block-2__map img
	height auto
	max-width 100%
.section--block-2__map
	margin-bottom -5px
.row + img
	margin-top 30px     
.section--block-3
	background url('../images/about-bg.jpg') 0 0 no-repeat
.section--block-3, .section--block-3
	background-size cover
.section--block-7_tabs-content
	display none
.section--block-7_tabs-content.active
	display block
.section--block-3
	text-align center
	color #fff
	h2
		font-size 40px
		line-height 48px
		margin-bottom 20px
	h2 + p
		font-size 22px
		line-height 30px
		max-width 1090px
		margin 0 auto
.section--block-3__box
	margin-top 35px
	box-shadow 0px 30px 125px 0 $dark-indigo--40
	background-color: #FFF
	text-align left
	color $denim
	padding 20px
	position relative
	margin-bottom calc(-85px - 96px)
	p
		font-size 18px
		line-height 22px
		color $blue-grey
	h3
		font-family bold
		color denim
		font-size 20px
		line-height 24px
		margin-bottom 0px
.section--block-4
	padding-top 150px
	h2
		color $denim-50
		font-size 40px
		line-height 48px
	p
		color $denim
		font-size 16px
		line-height 22px
	h3
		color $denim
		font-size 20px
		line-height 24px
		font-family bold
		margin-left 0px
.section--block-4__box
	background #fff
	border 10px solid #fff
	box-shadow inset 0 0 0 5px $pale-grey
	padding 30px
	ul
		column-count 1
		column-gap(120px)
		margin 0
		padding 0
		margin-left 20px
		list-style none
		line-height 22px
		font-size 20px
		color $denim
		li
			max-width 440px
			position relative
			margin-bottom 15px
			&:before
				content ''
				absolute(left -38px top 10px)
				width 25px
				height 2px
				background $pinkish-red-two
.section.section--block-5
	padding-top 0
	h2
		padding-bottom 20px
		border-bottom 3px solid $pinkish-red-two
		font-size 40px
		line-height 30px
		color $denim-50
		margin-bottom 45
		
	table
		tr
			td
				border-top 1px solid $bluey-grey
				color $denim
				font-size 20px
				padding 10px
				&:last-child
					font-family bold
					text-align right
		&:first-child
			td
				border-top 0
	img
		max-width 100%
		margin-top 0px
.section.section--block-6
	padding-top 20px
	padding-bottom 60px
	.section--block-6__box
		box-shadow 0px 30px 125px 0 $dark-indigo--40
		padding 10px
		img
			max-width 100%
	h2
		color $denim-50
		font-size 40px
		line-height 48px
		margin-bottom 0px
		margin-top 0
	p
		font-family bold
		font-size 20px
		line-height 24px
		color $denim
.section.section--block-7
	padding-top 30px
	padding-bottom 0px
	background-color $dark-grey-blue
	color #fff
	img
		max-width 100%
	h2
		border-bottom 1px solid $dusk
		font-size 30px
		line-height 48px
		padding-bottom 30px
.section--block-7__tabsbox
	background-color: #fff
	padding 10px
	box-shadow 0px 30px 145px 0 rgba(0, 5, 38, 0.5)
	margin-left 0px
	margin-bottom 0px
	img
		max-width 100%
.section--block-7_tabs-nav
	padding-left 0
	relative(left 0px)
	display block
	justify-content space-between
	align-items center
	max-width 1005px
	margin-bottom 20px
	div
		display flex
	a
		width 61px
		height 61px
		border-radius 100%
		display inline-block
		border 1px solid $light-blue-grey-three
		margin-left 10px
		color $blue-grey-two
		font-size 20px
		text-align center
		line-height 61px
		margin-top 10px
		&:first-child
			margin-left 0
	a.active
		border-color $pinkish-red-two
		color #fff
	.stock
		color $blue-grey-two
		font-size 20px
		font-family bold
.section--block-7__legend
	margin-top 20px
	ul
		list-style none
		font-size 20px
		line-height 100%
		color $blue-grey-two
		padding-left 0
		margin-top 40px
		display flex
		justify-content space-between
		flex-wrap wrap
		li
			position relative
			margin-bottom 30px
			padding-left 60px
			flex-basis 100%
			&:before
				content ''
				width 40px
				height 40px
				absolute(left 0 top -15px)
				border-radius 20px
		li.l-1
			&:before
				background-color $straw
				box-shadow 0px 15px 40px 0 $straw-70, inset 0px 1px 0 0 $white-six-60
		li.l-2
			&:before    
				background-color $light-blue-grey-four
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
		li.l-3
			&:before
				background-color $very-light-pink
				box-shadow 0px 15px 40px 0 $midnight-blue-60, inset 0px 1px 0 0 $very-light-pink-60
		li.l-4
			&:before
				background-color $beige-two
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
		li.l-5
			&:before
				background-color $beige-two
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
		li.l-6
			&:before
				background-color rgba(255,255,255,0.6)
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
		li.l-7
			&:before
				background-color rgba(255,255,255,0.6)
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
		li.l-8
			&:before
				background-color $pale-grey-two
				box-shadow 0px 15px 40px 0 $light-blue-grey-60, inset 0px 1px 0 0 $white-six-60
	h3
		font-size 24px
		line-height 28px
		margin-top 0
.stocks-img
	display none
// .section--block-8__slider-item
// 	padding 100px
.section--block-8__imgHolder
	padding 20px
	img
		box-shadow 0px 40px 100px 0px rgba(21, 40, 96, 0.3)	
		max-width 100%
.section--block-8__slider
	height auto
	.slick-arrow
		top auto
		bottom 75px
		z-index 200
		width 41px
		height 41px
		background-color: $denim
	.slick-prev
		right 60%
		left auto
		&:before
			content url('data:image/png;base64,')
	.slick-next
		right 30%
		left auto
		&:before
			content url('data:image/png;base64,')
.section--block-8
	position relative
	overflow hidden
	padding-top 30px
	&:before
		content none
	h3
		font-family bold
		font-size 24px
		line-height 1.27
		color $denim
.section--block-8__head
	display flex
	justify-content center
	margin-bottom 0
	h2
		text-align center
		color $denim-50
		font-size 40px
		line-height 0.96
		max-width 100%
		padding-left 0px
.section--block-8__sliderHead
	padding-top 0px
	padding-left 0px
	height auto
	max-width 390px
	position relative
	top 30px
	margin auto
	text-align center
.section--block-8__sliderNav-outer
	width 100%
	height 75px
	z-index 300
	.section--block-8__sliderNav-item
		width 100px
		text-align center
		img
			margin auto
.section--block-9
	padding-top 30px
	text-align center
	h2
		color $denim-50
		font-size 30px
		line-height 38px
		margin-top 0
.section--block-9__box
	background-color $denim
	box-shadow 0px 40px 120px 0 $denim-60
	padding 10px
	display flex
	flex-wrap wrap
	color #fff
	justify-content space-between
	align-items center
	text-align left
	font-size 16px
	line-height 15px
	.box__contact
		color $bluey-grey
.section--block-9__boxImg
	flex 0 0 auto
	margin 0 auto
.section--block-9__boxName
	flex 0 0 auto
	p
		white-space wrap
.section--block-9__boxPart
	flex 0 0 auto
	padding-top 30px

